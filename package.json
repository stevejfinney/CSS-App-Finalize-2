{
  "name": "css",
  "version": "0.0.0",
  "main": "main.js",
  "bin": "server.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json",
    "build": "ng build",
    "front": "ng test --reporters=junit --watch=false --browsers=ChromeHeadless",
    "test": "mocha --reporter mocha-junit-reporter --exit",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:all": "ng build && node server.js",
    "start:dev": "concurrently \"npm run watch\" \"npm run start\"",
    "mock_test": "node ./mock_test.js",
    "pro": "webdriver-manager update --versions.chrome=111.0.5563.146 && protractor e2e/protractor.conf.js",
    "exe_create": "pkg . --debug ",
    "target_file": "./node_modules/.bin/node-pre-gyp install --directory=./node_modules/sqlite3 --target_platform=win32 --target_arch=x64",
    "watch": "nodemon ./server.js",
    "start:production": "node server.js >> my_app_log.log 2>> my_app_err.log",
    "postinstall": "cd reporting  && npm install --force",
    "build_local":"ng build --prod --localize"
    
  },
  "private": true,
  "dependencies": {
    "@angular/forms": "^11.0.0 || ^12.0.0-0",
